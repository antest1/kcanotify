                <RelativeLayout
                    android:id="@+id/view_excheck_detail"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:minWidth="@dimen/excheckview_popup_width"
                    android:orientation="vertical"
                    android:padding="2dp">

                    <LinearLayout
                        android:id="@+id/excheck_detail_head"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/view_excheck_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="@dimen/popup_text_large" />

                        <TextView
                            android:id="@+id/view_excheck_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="00:00:00"
                            android:textColor="@color/white"
                            android:textSize="@dimen/popup_text_small" />

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/excheck_detail_line"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/excheck_detail_head"
                        android:layout_gravity="left|clip_vertical"
                        android:layout_marginTop="1dp"
                        android:layout_marginBottom="1dp"
                        android:layout_weight="0"
                        android:background="@color/grey"
                        android:paddingLeft="5dp"
                        android:paddingTop="0dp"
                        android:paddingRight="5dp"
                        android:paddingBottom="0.5dp"
                        android:scaleType="fitXY"
                        android:src="@android:drawable/divider_horizontal_bright" />

                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/excheck_detail_line"
                        android:layout_marginTop="4dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/view_excheck_flagship"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

