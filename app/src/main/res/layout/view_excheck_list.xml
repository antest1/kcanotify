                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="4dp"
                                        android:layout_marginRight="4dp"
                                        android:layout_weight="1"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/popup_text_normal" />

                                    <TextView
                                        android:id="@+id/view_excheck_flagship_cond"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="4dp"
                                        android:layout_marginRight="4dp"
                                        android:layout_weight="1"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/popup_text_normal" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/view_excheck_fleet"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/view_excheck_fleet_label"
                                        android:layout_width="@dimen/excheckview_label_width"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="4dp"
                                        android:layout_marginRight="4dp"
                                        android:text="@string/excheckview_total_info"
                                        android:textAlignment="center"
