                                            android:orientation="vertical">

                                        </LinearLayout>

                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="3"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:id="@+id/view_excheck_firepower"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/view_excheck_firepower_label"
                                            android:layout_width="@dimen/excheckview_label_width"
                                            android:layout_height="wrap_content"
                                            android:layout_marginEnd="4dp"
                                            android:layout_marginRight="4dp"
                                            android:text="@string/excheckview_firepower_info"
                                            android:textAlignment="center"
                                            android:textColor="@color/white"
                                            android:textSize="@dimen/popup_text_normal" />

                                        <LinearLayout
                                            android:id="@+id/view_excheck_firepower_data"
                                            android:layout_width="match_parent"
